fun main() {
    
   val car = Car()
   println(car.color)
   println(car.model)
   print(car.drive())
  
  println("Car color: ${car.color} model: ${car.model}")
}

class Car {
    var color: String = "Red"
    var model: String = "XMD"
    
    
    fun drive() {
        println("Drive...vroomm")
    }
}
